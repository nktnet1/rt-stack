{
  "name": "web",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"src/**/*.ts\"",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "clean": "git clean -xdf .cache .next .turbo node_modules",
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "start": "vite"
  },
  "dependencies": {
    "@tailwindcss/vite": "catalog:",
    "@tanstack/react-router": "catalog:",
